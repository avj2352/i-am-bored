{
  "name": "i-am-board-api",
  "version": "1.0.0",
  "description": "rest api for bored recipe app",
  "main": "index.js",
  "author": "pramod",
  "license": "MIT",
  "engines": {
    "node": "^12.15.0",
    "npm": "^6.13.4"
  },
  "scripts": {
    "start": "node ./dist/index.js",
