{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "latest",
    "@date-io/moment": "1.1.0",
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.36",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/history": "^4.7.8",
    "axios": "^0.19.0",
    "ckeditor4-react": "^1.3.0",
    "clsx": "^1.1.0",
    "date-fns": "next",
    "gsap": "^3.2.0",
    "material-ui-pickers": "latest",
    "moment": "2.24.0",
    "notistack": "^0.8.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "react-pose": "^4.0.10",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-sortablejs": "^2.0.9"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@types/react-dom": "^16.9.5",
    "cross-env": "^5.2.0",
    "jest-junit": "^10.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "typescript": "^3.8.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**",
      "!node_modules/**"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:final": "npm run prettier:format && react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "https://justbored.herokuapp.com",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
